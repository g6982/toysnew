<?xml version="1.0"?>
<odoo>
    <record id="view_move_line_comission_bank_tree" model="ir.ui.view">
        <field name="name">account.account.comission.bank.tree</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ref']" position="after">
                <field name="move_comission_id" optional="show"/>
                <field name="forma_pago_id" optional="show"/>
            </xpath>
        </field>
    </record>

</odoo>
