<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ywt_res_company_form_view_inherit" model="ir.ui.view">
        <field name="name">ywt.res.company.form.view.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='website']" position="after">
                <field name="bridge_transfer_location_id" options="{'no_create':True}"/>
            </xpath>
        </field>
    </record>
</odoo>
